<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">

    <TextView
        android:id="@+id/item_msg_logId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="生成时间：2020.12.12  15:30:22"
        android:textColor="#333333"
        android:textSize="16dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/item_msg_userId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="用户ID：45"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:visibility="gone"
        android:text="记录类型：安装app收到广播"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_gameId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="游戏ID：45"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_orderId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="订单ID：45"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_packageNames"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="包名：comdsfsdf,sdfsd,sdf"
        android:textColor="#666666"
        android:textSize="14dp" />
    <TextView
        android:id="@+id/item_msg_localAllTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="本地任务时长：10000"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_localMsg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="查询本地任务结果：查询成功"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_queryStartTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="本次记录查询开始时间：0"
        android:visibility="gone"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_queryEndTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:visibility="gone"
        android:text="本次记录查询结束时间：2020.20.20 22:00:00"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_querySystemTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="本次记录查询结果时间：10000"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_localCacheDataTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="未上报缓存表时长：10000"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_zipLocalAndQueryTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="组装缓存后时长：10000"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_queryMsg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="本次查询系统时间结果：成功"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_reportHttpResults"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="接口上报结果：code 0 msg 1"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_reportHttpMsg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="联网上报结果：陈工"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_reportOver"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="上报完毕：陈工"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_error"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="出现错误：陈工"
        android:textColor="#666666"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/item_msg_competence"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingBottom="10dp"
        android:text="有没有权限：true"
        android:textColor="#666666"
        android:textSize="14dp" />

</LinearLayout>